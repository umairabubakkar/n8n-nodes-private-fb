{
  "name": "@umairabubakkar/n8n-nodes-private-fb",
  "version": "1.3.7",
  "description": "Private Facebook Graph API integration for n8n with enhanced AI Agent logging. Complete automation suite with Posts, Comments, Messages, Insights, Events, Media & Real-time Triggers. OAuth2 authentication with comprehensive error handling and structured AI Agent logs.",
  "main": "dist/FacebookPagePublisher.node.js",
  "types": "dist/FacebookPagePublisher.node.d.ts",
  "files": [
    "dist/**/*",
    "credentials/**/*",
    "README.md",
    "TROUBLESHOOTING.md",
    "FACEBOOK_CREDENTIALS_GUIDE.md",
    "QUICK_FIX_GUIDE.md",
    "FIXES_APPLIED.md",
    "CHANGELOG.md",
    "AI_AGENT_LOGGING_GUIDE.md",
    "CREATE_COMMENT_WITH_IMAGE_GUIDE.md",
    "IMAGE_UPLOAD_FIX_GUIDE.md",
    "RELEASE_NOTES_v1.3.2.md",
    "FacebookPagePublisher.svg",
    "facebook.svg",
    "TOKEN_MANAGEMENT_GUIDE.md",
    "AI_TOOL_GUIDE.md",
    "examples/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run copy-assets",
    "copy-assets": "copy facebook.svg dist\\facebook.svg && copy FacebookPagePublisher.svg dist\\FacebookPagePublisher.svg",
    "prepublishOnly": "npm run build",
    "test": "jest --config test/jest.config.js",
    "test:watch": "jest --config test/jest.config.js --watch",
    "test:coverage": "jest --config test/jest.config.js --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "release:patch": "npm run version:patch && npm run build && npm publish",
    "release:minor": "npm run version:minor && npm run build && npm publish",
    "release:major": "npm run version:major && npm run build && npm publish",
    "quick-release": "npm run version:patch && npm run build && npm publish"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "facebook",
    "facebook-api",
    "social-media",
    "automation",
    "workflow",
    "integration",
    "oauth2",
    "graph-api",
    "page-publisher",
    "social-posting",
    "triggers",
    "webhooks",
    "real-time",
    "ai-agent",
    "ai-tool",
    "logging",
    "langchain"
  ],
  "author": {
    "name": "Umair Abu Bakkar",
    "email": "umairabubakkar@example.com",
    "url": "https://github.com/umairabubakkar"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umairabubakkar/n8n-nodes-private-fb.git"
  },
  "bugs": {
    "url": "https://github.com/umairabubakkar/n8n-nodes-private-fb/issues"
  },
  "homepage": "https://github.com/umairabubakkar/n8n-nodes-private-fb#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FacebookOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/FacebookPagePublisher.node.js",
      "dist/FacebookPageTrigger.node.js",
      "dist/FacebookTool.node.js"
    ]
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}